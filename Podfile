# Uncomment the next line to define a global platform for your project
# platform :ios, '9.0'

target 'relliK' do
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!
  ##Continious Integration Buddy Build
  pod 'BuddyBuildSDK'
  ##Google ads
  pod 'Google-Mobile-Ads-SDK'
  ##Confetti
  pod 'SAConfettiView'
  ##Onboarding
  pod 'Onboard'
  ##Swift lint
  pod 'SwiftLint'
  ##Flurry analytics
  pod 'Flurry-iOS-SDK/FlurrySDK'
  ##Amplitude analytics
  pod 'Amplitude-iOS'
  ##Google ads
  pod 'Google-Mobile-Ads-SDK'
  #Debug Logger with tags
  pod 'SwiftyBeaver'
  ##(1.11.4) Ask's user to rate app after x amount of days
  pod 'iRate'
  ##This will be done when the app has an app id number
  # Pods for relliK
  pod 'Siren'

  target 'relliKTests' do
    inherit! :search_paths
    # Pods for testing
  end

  target 'relliKUITests' do
    inherit! :search_paths
    # Pods for testing
  end

end
